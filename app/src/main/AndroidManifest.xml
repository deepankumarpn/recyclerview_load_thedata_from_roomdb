<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dk.productlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Vxceed">
        <activity
            android:name="com.dk.productlist.ProductAdd"
            android:label="Add &amp; Edit"
            android:parentActivityName="com.dk.productlist.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dk.productlist.MainActivity" />
        </activity>
        <activity
            android:name="com.dk.productlist.ProductDetails"
            android:label="Details "
            android:parentActivityName="com.dk.productlist.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dk.productlist.MainActivity" />
        </activity>
        <activity android:name="com.dk.productlist.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>